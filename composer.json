    "autoload": {
        "files": [
          "registration.php"
        ],
        "psr-4": {
            "MageForge\\QuickIndex\\": ""
        }
    },
    "extra": {
        "map": [
            [
                "*",
                "MageForge/QuickIndex"
            ]
        ]
    }
}
